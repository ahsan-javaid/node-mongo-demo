{
  "specialization": {},
  "middleware": {
    "static": {
      "module": {
        "arguments": ["path:./.build"]
      }
    },
    "router": {
      "module": {
        "arguments": [
          {
            "directory": "path:./app/controllers"
          }
        ]
      }
    },
    "response-sender": {
      "enabled": true,
      "priority": 70,
      "route": "/api",
      "module": {
        "name": "path:./app/lib/response/responseAppender"
      }
    },
    "authentication": {
      "enabled": true,
      "priority": 100,
      "route": "/api/v1((?!$))((?!/categories))((?!/posts))((?!/users/signup))((?!/users/forgot))((?!/users/change))((?!/users/activate-code))((?!/users/login))*",
      "module": {
        "name": "path:./app/lib/auth"
      }
    }
  },
  "app": {
    "url": "",
    "jwtSecret": "d-vour-secret"
  }
}
